{
  "compilerOptions": {
      "target": "es2015",
      "module": "es2015",
      "declaration": true,
      "outDir": "./dist",
      "baseUrl": ".",
      "paths": {
          "@/*": [
              "./src/*"
          ]
      },
    //   "importHelpers": true,
      "moduleResolution": "node",
      "strict": true,
      "noImplicitAny": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
  }
}